type Query {
    _health: String
}

type Mutation {
    createAppointment(input: AppointmentInputDTO!): Appointment
}

input AppointmentInputDTO {
    patientId: ID!
    doctorId: ID!
    appointmentDateTime: String!
    status: String!
}

type Appointment {
    id: ID!
    patientId: ID!
    doctorId: ID!
    appointmentDateTime: String!
    status: String!
}